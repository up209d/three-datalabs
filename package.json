    "raw-loader": "0.5.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.0",
    "react-jss": "8.4.0",
    "react-motion": "0.5.2",
    "react-pose": "1.6.4",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
