    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "speed-measure-webpack-plugin": "1.2.2",
    "style-loader": "0.20.3",
    "three": "0.92.0",
    "three-bas": "2.7.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
